{
  "executable": "GestThé",
  "splashscreen": {
    "imageFile": ""
  },
  "name": "GestThé",
  "author": "Sébastien Duruz",
  "singleInstance": true,
  "environment": "Production",
  "build": {
    "appId": "com.GestThé.app",
    "productName": "GestThé",
    "copyright": "Copyright © 2023",
    "buildVersion": "0.1.0",
    "compression": "maximum",
    "directories": {
      "output": "../../../bin/Desktop"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": [ "**/*" ]
      }
    ],
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": [ "**/*" ]
      },
      "**/*"
    ]
  }
}